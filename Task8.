#Particle WEB IDE
#include<Wire.h>
int led = D7;

void setup() {
    Wire.begin(0x8);
    Wire.onReceive(receiveEvent);
    
    
    pinMode(led, OUTPUT);
    digitalWrite(led, LOW);

}

void receiveEvent(int i){
    while (Wire.available()){
        char c = Wire.read();
        digitalWrite(led, c);
    }
}
void loop() {
    delay(1000);
}
